

<div class="container-fluid">
<div class="row-fluid">
<div class="span12">

    <div class="page-header">
        <h3>Denuncias</h3>
        <p>Listado de las Ãºltimas denuncias</p>
    </div>
    <% denuncias.forEach((denuncia) => { %>

    <div class="carousel slide" id="myCarousel">
        <div class="carousel-inner">
            <div class="item active">
                    <ul class="thumbnails">
                        <li class="span3">
                            <div class="thumbnail">
                                <a href="#"><img src="<%= denuncia.imgUrl %>"alt=""></a>
                            </div>
                            <div class="caption">
                                <h4><%= denuncia.subject %></h4>
                                <span ><%= denuncia.showName %></span>
                    			      <p><%= denuncia.description %></p>
                                <a href="/denuncias/<%=denuncia._id%>">Read More</a>
                            </div>
                        </li>

    </div><!-- /#myCarousel -->
  <%  }) %>
</div><!-- /.span12 -->
</div><!-- /.row -->
</div><!-- /.container -->
